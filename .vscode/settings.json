{
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.formatOnType": true,
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "python.defaultInterpreterPath": "./.venv/bin/python",
    "python.analysis.typeCheckingMode": "standard",
    "python.autoComplete.extraPaths": [
        "./.venv/lib/python3.11/site-packages",
        "./.venv/lib64/python3.11/site-packages",
        "/usr/lib/x86_64-linux-gnu/python3.11/site-packages"
    ],
    "python.analysis.extraPaths": [
        "./.venv/lib/python3.11/site-packages",
        "./.venv/lib64/python3.11/site-packages",
        "/usr/lib/x86_64-linux-gnu/python3.11/site-packages",
        "./master",
        "./camera",
        "./common"
    ],
    "python.analysis.logLevel": "Information",
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.useLibraryCodeForTypes": true,
    "java.project.referencedLibraries": [
        "connector/lib/**/*.jar",
        "connector/lib/metashape-java-api.jar",
        "connector/lib/metashape-java-api-javadoc.jar",
        "connector/lib/metashape-java-api-sources.jar"
    ],
    "java.project.sourcePaths": [
        "connector/src"
    ],
    "java.project.outputPath": "connector/bin",
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
}